package com.mygdx.game;

import com.badlogic.gdx.Gdx;
import com.badlogic.gdx.graphics.Texture;

public class Player {
	float x,y;
	
	Texture tex;

	public Player(float x, float y, Texture tex) {
		this.x = x;
		this.y = y;
		this.tex = tex;
	}

	public Player(Texture tex) {
		Gdx.app.log("User","Pos not set");
		this.x = 0;
		this.y = 0;
		this.tex = tex;
	}
	
	
}
