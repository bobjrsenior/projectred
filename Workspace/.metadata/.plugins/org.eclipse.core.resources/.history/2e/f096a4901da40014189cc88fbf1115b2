package com.mygdx.game;

import com.badlogic.gdx.ApplicationListener;
import com.badlogic.gdx.Game;

public class SceneManager extends Game implements ApplicationListener {
	private Game game;

	public SceneManager() {
		game = this;
		
	}

	@Override
	public void create() {
		setScreen(new TitleScreen(game));
		
	}

}
